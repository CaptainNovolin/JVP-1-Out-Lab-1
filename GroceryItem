public class GroceryItem
{
    private String name;
    private int cost;
    private int aisleNum;
    
    public GroceryItem(String startName, int startCost, int startAisleNum)
    {
       name = startName;
       cost = startCost;
       aisleNum = startAisleNum;
    }
    
    public int getName()
    {
        return startName;
    }
}
