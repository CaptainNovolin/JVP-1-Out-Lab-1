public class GroceryItem
{
    private String name;
    private double cost;
    private int aisleNum;
    
    public GroceryItem(String startName, double startCost, int startAisleNum)
    {
       name = startName;
       cost = startCost;
       aisleNum = startAisleNum;
    }
    
    public String getName()
    {
        return name;
    }
    
    public double getCost()
    {
        return cost;
    }
    
    public int getAisle()
    {
        return aisleNum;
    }
}

